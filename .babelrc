{
  "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@components": "./src/components",
        "@middleware": "./src/middleware",
        "@services": "./src/services",
        "@store": "./src/store",
        "@utils": "./src/utils",
        "@styles": "./src/styles"
      }
    }]
  ]
}